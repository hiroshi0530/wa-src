    "import json\n",
    "\n",
    "with open(file_name, mode='r') as f:\n",
    "  for line in f.readlines():\n",
    "    info = json.loads(line)\n",
    "    if info['title'] == 'イギリス':\n",
    "      print('### 先頭500文字 ###')\n",
    "      print(info['text'][:500])\n",
    "      print()\n",
    "      print('### 最後500文字 ###')\n",
    "      print(info['text'][-500:])\n",
    "      \n",
    "      # 以後 b_infoをイギリスの情報として利用\n",
    "      b_info = info['text']"
   ]
